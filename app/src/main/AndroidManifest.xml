<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tellh.recyclerviewdemo">

    <application
        android:name=".AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.StaggeredActivity" />
        <activity android:name=".activity.AdapterTestActivity" />
        <activity android:name=".activity.ItemSwipeAndDragActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.NewWayToAddHeaderActivity" />
        <activity android:name=".activity.PopupMenuActivity" />
        <activity android:name=".activity.RecyclerRefreshActivity" />
        <activity android:name=".activity.RvEmptyViewActivity" />
        <activity android:name=".activity.ToolbarAnimatedActivity" />
        <activity android:name=".activity.StickyHeaderActivity"
            android:theme="@style/StickyActivityTheme"/>
        <activity android:name=".activity.DiffUtilActivity"
            android:label="@string/app_name"
            android:theme="@style/DiffActivityTheme"/>

        <activity android:name=".activity.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>